<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
        
<mapper namespace="com.mep.sitemap.mapper.SiteMapMapper">
	<select id="getAllArticles" resultType="com.mep.sitemap.dto.SiteMapDto">

		SELECT
			post.post_title_eng,
			post.created_date
		FROM
			post
	</select>
</mapper>